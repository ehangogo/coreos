Bundle-Version:					1.0.0
Bundle-Description: 				\
	A bundle with a provider. Notice that this provider exports the API package. \
	It also provides a JUnit test and it can be run standalone. \
	\
	${warning;Please update this Bundle-Description in os.moudel.guard.provider/bnd.bnd}

Private-Package:  \
	os.moudel.guard.provider
	
-buildpath: \
	osgi.enroute.base.api;version=1.0,\
	os.core.provider;version=latest
	
-testpath: \
	osgi.enroute.junit.wrapper;version=4.12

-includeresource: {readme.md}


-runrequires:  \
	osgi.identity;filter:='(osgi.identity=os.moudel.guard.provider)'
	
-runbundles: \
	${error;Resolve first}
	
Bundle-Icon: icon.png;size=64
Bundle-Name: 监护人管理
Bundle-Description: 光彩健康监控系统，提供了医生管理，亲属管理，志愿者管理等功能
Bundle-Vendor: 尹行欣
Bundle-Copyright: 尹行欣
